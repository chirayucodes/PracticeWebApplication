@model IEnumerable<StudentViewModel>

<table class="table table-striped table-bordered">
    <thead>
    <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>FatherName</th>
        <th>MotherName</th>
        <th>Gender</th>
        <th>Address</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var s in Model)
    {
        <tr>
            <td>@s.ID</td>
            <td>@s.StudentName</td>
            <td>@s.FatherName</td>
            <td>@s.MotherName</td>
            <td>@s.Gender</td>
            <td>@s.Address</td>
        </tr>
    }
    </tbody>
</table>